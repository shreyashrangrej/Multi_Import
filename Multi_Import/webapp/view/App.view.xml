<mvc:View controllerName="multiMulti_Import.controller.App" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Button text="Import" press="onImport" />
					<Table id="idTable" items="{/}">
						<columns>
							<Column>
								<Label text="Name" />
							</Column>
							<Column>
								<Label text="Age" />
							</Column>
							<Column>
								<Label text="Gender" />
							</Column>
						</columns>
						<ColumnListItem>
							<cells>
								<Input value="{Name}" />
								<Input value="{Age}" />
								<Input value="{Gender}" />
							</cells>
						</ColumnListItem>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>